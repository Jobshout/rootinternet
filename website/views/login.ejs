<% include include/main-header %>
</head>
<body>
 	<!--header end-->
	<% include include/header %>
	<!--header end-->
 	<div class="breadcrumbs">
      <div class="container">
        <div class="row">
          <div class="col-lg-4 col-sm-4">
           <h1 class="mainheading"><span>Please Log In</span></h1>
          </div>
          <div class="col-lg-8 col-sm-8">
            <ol class="breadcrumb pull-right">
              <li>
                <a href="/">
                  Home
                </a>
              </li>
              
              <li class="active">
              	Log In
              </li>
            </ol>
          </div>
        </div>
      </div>
    </div>
	<div class="container">
    	<div CLASS="row">
			<div class="col-lg-12 col-sm-12 address content">
          		<h4> Log in to the monitoring control panel</h4>
          		<div class="panel-body">
					<form class="form-horizontal" action="http://monitor.rootinternet.co.uk/no/login.php" id="loginForm" method="post" name="loginForm">
             			<div class="form-group">
							<label for="login_username name" class="col-sm-4  col-md-2 control-label">Enter your Username<sup class="red-text">*</sup></label>
                  			<div class="col-sm-8 col-md-8">
                    			<input class="form-control col-sm-8" type="text" name="login_username">
                  			</div>
                		</div>
                		<div class="form-group">
                  			<label for="" class="col-sm-4  col-md-2 control-label">Password<sup class="red-text">*</sup></label>
                  			<div class="col-sm-8 col-md-8">
                    			<input class="form-control" type="text" name="password">
                  			</div>
                		</div>
                		<div class="form-group">
   					 		<div class="col-sm-offset-3 col-md-offset-2 col-sm-1 col-md-2">
      							<button type="submit" class="btn btn-primary">Log In</button>
    				 		</div>
                    		<div class="col-sm-12 col-md-7">
                     			<p class="small text-right  mrgn-lft-zero" style="margin-top:5px;">Complete all fields marked <sup class="red-text">*</sup></p>
                     		</div>
  						</div>
               		</form>
				</div>
        	</div>
		</div>
	</div>
<!--footer start-->
    <% include include/footer %>
<!--footer end-->
<script src="js/jquery.validate.js"></script>
<script>
$(document).ready(function() {
	$("#loginForm").validate({
		rules: {
			login_username: "required",
			password: "required"
		},
		messages: {
			login_username: "Please enter your username",
			password: "Please enter password"
		}
	});
});
</script>
  </body>
</html>